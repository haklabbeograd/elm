  "main": "src/main.elm",
  "scripts": {
    "test": "echo \"LOL TESTS!\" && exit 9000",
    "start": "concurrently -k -p name -n \"host,elm,styl\" -c \"grey,cyan,magenta\" \"npm run serve\" \"npm run watch-elm\" \"npm run watch-styl\"",
